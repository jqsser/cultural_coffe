<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.collections.FXCollections?>
<?import java.lang.String?>
<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="Controllers.ModifierProduitBack" prefHeight="400.0" prefWidth="600.0">

    <ComboBox fx:id="etatProduitComboBox" layoutX="100" layoutY="200">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="oui"/>
                <String fx:value="non"/>
            </FXCollections>
        </items>
    </ComboBox>
    <Button fx:id="modifierButton" layoutX="100" layoutY="250" text="Modifier" onAction="#modifierProduit"/>
</AnchorPane>
